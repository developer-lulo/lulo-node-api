{
  "name": "lulo-node-api",
  "version": "1.5.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prebuild": "rm -fr dist",
    "build": "yarn prebuild && tsc",
    "start": "node --enable-source-maps --experimental-specifier-resolution=node ./dist/src/index.js",
    "dev": "nodemon --watch src --ext '*' --exec 'yarn build && yarn start'",
    "build:to:zip": "yarn build && sh scripts/zip-build-directory.sh",
    "build:to:bundle": "yarn build && node scripts/build-bundle.js",
    "codegen": "NODE_OPTIONS=\"--loader ts-node/esm\" graphql-codegen-esm --config codegen.yml",
    "migrations": "tsc --project tsconfig.sequelize.json && sh scripts/add-commonjs-package-json.sh && sequelize-cli db:migrate",
    "migrations:undo": "tsc --project tsconfig.sequelize.json && sh scripts/add-commonjs-package-json.sh && sequelize-cli db:migrate:undo"
  },
  "keywords": [
    "graphql",
    "nodejs",
    "typescript",
    "sequelize"
  ],
  "author": "Felipe Mantilla",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.6.0",
    "@google-cloud/storage": "^6.11.0",
    "@graphql-codegen/typescript": "^2.8.3",
    "@graphql-codegen/typescript-resolvers": "^2.7.8",
    "@graphql-tools/schema": "^9.0.17",
    "@graphql-tools/utils": "^9.1.1",
    "apollo-server-core": "^3.12.0",
    "apollo-server-express": "^3.11.1",
    "archiver": "^5.3.1",
    "aws-sdk": "^2.1350.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.14",
    "express": "^4.18.2",
    "express-jwt": "8.4.1",
    "graphql": "^16.6.0",
    "graphql-redis-subscriptions": "^2.6.0",
    "graphql-resolvers": "^0.4.2",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.12.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.26.0",
    "sequelize-typescript": "^2.1.5",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.15.0",
    "@graphql-codegen/introspection": "2.2.1",
    "@types/node": "^18.13.0",
    "@types/validator": "^13.7.12",
    "babel-register": "^6.26.0",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.2",
    "tsc": "^2.0.4",
    "typescript": "^4.9.3"
  }
}
